{
	"bin": {
		"wtf": "./dist/cli/index.js"
	},
	"dependencies": {
		"@types/node": "^16.7"
	},
	"devDependencies": {
		"typescript": "^4.0"
	},
	"license": "MIT",
	"main": "./dist/lib/index.js",
	"name": "@joelek/wtf",
	"repository": {
		"type": "git",
		"url": "https://github.com/joelek/wtf.git"
	},
	"scripts": {
		"appgen": "node -p \"JSON.stringify((({ name, version }) => ({ name, version }))(require('./package.json')), null, '\t')\" > ./source/app.json",
		"build": "npm run appgen && npm run compile && npm run package && npm run test",
		"clean": "rm -rf ./build/ ./node_modules/",
		"compile": "rm -rf ./build/* && npx tsc --build",
		"package": "rm -rf ./dist/* && cp -r ./build/* ./dist/",
		"version": "npm run build && git add ./",
		"postversion": "git push && git push --tags",
		"test": "node ./dist/cli/index.js"
	},
	"version": "1.3.1"
}
